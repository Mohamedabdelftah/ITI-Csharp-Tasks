using System;
struct Program{
	static void Main (){
		Console.WriteLine("Hello My Friend Enter The First Number Please");
		int x = int.Parse(Console.ReadLine());
		Console.WriteLine("Hello My Friend Enter The Second Number Please");
		int y = int.Parse(Console.ReadLine());
		int sum =  x + y ;
		int sub =  x - y ;
		int mult=  x * y ;
		int divi=  x / y ; 
		Console.WriteLine("The results of processe :Sum = {0}, Sub = {1} , Multiply = {2} , Divid = {3} :",sum , sub , mult , divi);
		
		
		Console.ReadLine();
	}
	
}
